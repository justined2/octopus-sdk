{
  "name": "@saooti/octopus-sdk",
  "version": "37.0.9",
  "private": false,
  "description": "Javascript SDK for using octopus",
  "author": "Saooti",
  "scripts": {
    "serve": "vite preview",
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint --ext .js,.vue --fix src",
    "build_bundle": "vue-cli-service build --target lib --name octopus ./index.js",
    "sonar": "node sonarqube-scanner.js",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@saooti/octopus-api": "^0.34.3",
    "@tiptap/extension-link": "^2.0.4",
    "@tiptap/extension-underline": "^2.0.4",
    "@tiptap/pm": "^2.0.4",
    "@tiptap/starter-kit": "^2.0.4",
    "@tiptap/vue-3": "^2.0.4",
    "@vue/cli": "^5.0.8",
    "@vue/compat": "^3.3.4",
    "@vuepic/vue-datepicker": "^5.4.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "dayjs": "^1.11.9",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "express": "^4.18.2",
    "hls.js": "^1.4.9",
    "humanize-duration": "^3.29.0",
    "jest": "^29.6.1",
    "pinia": "^2.1.4",
    "qrcode.vue": "^3.4.0",
    "sass": "^1.63.6",
    "sonarqube-scanner": "^3.0.0",
    "swiper": "^10.0.4",
    "vite": "^4.4.4",
    "vue": "^3.3.4",
    "vue-i18n": "^9.2.2",
    "vue-recaptcha": "^2.0.3",
    "vue-router": "^4.2.4",
    "vue-select": "^4.0.0-beta.6",
    "vue3-lazyload": "^0.3.6",
    "vue3-swatches": "^1.2.3"
  },
  "devDependencies": {
    "@types/vue-select": "^3.16.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "typescript": "^5.1.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "@vue/typescript",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:vue/base",
      "plugin:vue/vue3-recommended",
      "plugin:vue/vue3-essential",
      "plugin:vue/vue3-strongly-recommended",
      "prettier",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-console": "off",
      "no-debugger": "off",
      "@typescript-eslint/ban-ts-ignore": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "no-async-promise-executor": "off",
      "prettier/prettier": [
        "error",
        {}
      ]
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bugs": {
    "url": "https://github.com/saooti/octopus-sdk/issues"
  },
  "homepage": "https://github.com/saooti/octopus-sdk#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saooti/octopus-sdk.git"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}
