{
  "name": "@saooti/octopus-sdk",
  "version": "36.0.2",
  "private": false,
  "description": "Javascript SDK for using octopus",
  "author": "Saooti",
  "scripts": {
    "serve": "vite preview",
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint --ext .js,.vue --fix src",
    "build_bundle": "vue-cli-service build --target lib --name octopus ./index.js",
    "prettifile": "prettier --single-quote --trailing-comma es5 --write",
    "prettify": "prettier --single-quote --trailing-comma es5 --write src/**/**/**/*.{js,vue}",
    "sonar": "node sonarqube-scanner.js",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.7",
    "@saooti/octopus-api": "^0.34.3",
    "@vue/cli": "^5.0.8",
    "@vue/compat": "^3.2.45",
    "autoprefixer": "^10.4.14",
    "axios": "^1.3.4",
    "dayjs": "^1.11.7",
    "express": "^4.18.2",
    "hls.js": "^1.3.5",
    "humanize-duration": "3.27.1",
    "jest": "^29.5.0",
    "jquery": "^3.6.4",
    "pinia": "^2.0.33",
    "qrcode.vue": "^3.3.4",
    "sass": "^1.60.0",
    "sonarqube-scanner": "^3.0.0",
    "swiper": "^8.4.7",
    "v-calendar": "^3.0.3",
    "vite": "^3.2.5",
    "vue": "^3.2.45",
    "vue-i18n": "^9.2.2",
    "vue-multiselect": "^3.0.0-beta.1",
    "vue-recaptcha": "^2.0.3",
    "vue-router": "^4.1.6",
    "vue3-lazyload": "^0.3.6",
    "vue3-swatches": "1.0.10"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.16",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/eslint-config-typescript": "^8.0.0",
    "eslint": "^8.37.0",
    "eslint-plugin-vue": "^7.17.0",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "@vue/typescript",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:vue/base",
      "plugin:vue/vue3-recommended",
      "plugin:vue/vue3-essential",
      "plugin:vue/vue3-strongly-recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-debugger": "off",
      "@typescript-eslint/ban-ts-ignore": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "no-async-promise-executor": "off"
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bugs": {
    "url": "https://github.com/saooti/octopus-sdk/issues"
  },
  "homepage": "https://github.com/saooti/octopus-sdk#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saooti/octopus-sdk.git"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}
