{
  "name": "@saooti/octopus-sdk",
  "version": "33.1.2",
  "private": false,
  "description": "Javascript SDK for using octopus",
  "author": "Saooti",
  "scripts": {
    "serve": "vite preview",
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "build_bundle": "vue-cli-service build --target lib --name octopus ./index.js",
    "prettifile": "prettier --single-quote --trailing-comma es5 --write",
    "prettify": "prettier --single-quote --trailing-comma es5 --write src/**/**/**/*.{js,vue}",
    "sonar": "node sonarqube-scanner.js",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@saooti/octopus-api": "^0.33.0",
    "@vue/cli": "^5.0.8",
    "@vue/compat": "^3.2.37",
    "autoprefixer": "^10.4.8",
    "axios": "^0.24.0",
    "bootstrap": "5.2.0",
    "express": "^4.18.2",
    "hls.js": "^1.1.5",
    "humanize-duration": "^3.27.2",
    "jest": "^29.1.2",
    "jquery": "^3.5.14",
    "moment": "^2.29.4",
    "qrcode.vue": "^3.3.3",
    "sass": "^1.53.0",
    "sonarqube-scanner": "^2.8.2",
    "swiper": "^8.3.1",
    "v-calendar": "^3.0.0-alpha.8",
    "vite": "^3.0.4",
    "vue": "^3.2.37",
    "vue-i18n": "^9.2.0-beta.40",
    "vue-multiselect": "^3.0.0-alpha.2",
    "vue-recaptcha": "^2.0.3",
    "vue-router": "^4.1.2",
    "vue3-lazyload": "^0.2.5-beta",
    "vue3-swatches": "^1.0.10",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.1.13",
    "@types/jquery": "^3.5.14",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/eslint-config-typescript": "^8.0.0",
    "eslint": "^8.20.0",
    "eslint-plugin-vue": "^7.17.0",
    "typescript": "^4.7.4"
  },
  "eslintConfig": {
    "parser": "vue-eslint-parser",
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "@vue/typescript",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:vue/base",
      "plugin:vue/vue3-recommended",
      "plugin:vue/vue3-essential",
      "plugin:vue/vue3-strongly-recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-debugger": "off",
      "@typescript-eslint/ban-ts-ignore": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "no-async-promise-executor": "off"
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bugs": {
    "url": "https://github.com/saooti/octopus-sdk/issues"
  },
  "homepage": "https://github.com/saooti/octopus-sdk#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saooti/octopus-sdk.git"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}
