{
  "name": "@saooti/octopus-sdk",
  "version": "31.0.26",
  "private": false,
  "description": "Javascript SDK for using octopus",
  "author": "Saooti",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --report",
    "lint": "vue-cli-service lint --fix",
    "build_bundle": "vue-cli-service build --target lib --name octopus ./index.js",
    "prettifile": "prettier --single-quote --trailing-comma es5 --write",
    "prettify": "prettier --single-quote --trailing-comma es5 --write src/**/**/**/*.{js,vue}"
  },
  "main": "./dist/octopus.common.js",
  "dependencies": {
    "@saooti/octopus-api": "^0.31.3",
    "@vue/cli": "^5.0.4",
    "@vue/compat": "^3.2.31",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "core-js": "^3.21.1",
    "hls.js": "^1.1.5",
    "humanize-duration": "^3.27.1",
    "jquery": "^3.5.14",
    "moment": "^2.29.2",
    "moment-locales-webpack-plugin": "^1.2.0",
    "qrcode.vue": "^3.3.3",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "swiper": "^8.1.5",
    "v-calendar": "^3.0.0-alpha.8",
    "vue": "^3.2.31",
    "vue-i18n": "^9.2.0-beta.34",
    "vue-multiselect": "^3.0.0-alpha.2",
    "vue-recaptcha-v3": "^2.0.1",
    "vue-router": "^4.0.14",
    "vue3-lazyload": "^0.2.5-beta",
    "vue3-swatches": "^1.0.7",
    "vuex": "^4.0.2",
    "webpack": "^5.71.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.1.8",
    "@types/jquery": "^3.5.14",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
    "@vue/cli-plugin-typescript": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/eslint-config-typescript": "^8.0.0",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^7.20.0",
    "typescript": "^4.6.3"
  },
  "eslintConfig": {
    "parser": "vue-eslint-parser",
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "@vue/typescript",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:vue/base",
      "plugin:vue/vue3-recommended",
      "plugin:vue/vue3-essential",
      "plugin:vue/vue3-strongly-recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-debugger": "off",
      "@typescript-eslint/ban-ts-ignore": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "no-async-promise-executor": "off"
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bugs": {
    "url": "https://github.com/saooti/octopus-sdk/issues"
  },
  "homepage": "https://github.com/saooti/octopus-sdk#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saooti/octopus-sdk.git"
  }
}
